parameter %default unit;
storage (list (or :value (bool %BVal) (int %ZVal)));
code { DUP ;
       DIP { CDR @_storage } ;
       CAR @_parameter ;
       LAMBDA @inner
         (pair timestamp (pair address (pair mutez mutez)))
         (option (list (or :value (bool %BVal) (int %ZVal))))
         { RENAME @ctx_slash_90 ;
           DUP @ctx ;
           PUSH (option (list (or :value (bool %BVal) (int %ZVal)))) (Some {}) ;
           DIP { DROP } ;
           DIP { DROP } } ;
       BALANCE ;
       AMOUNT ;
       PAIR ;
       SENDER ;
       PAIR ;
       NOW ;
       PAIR @ctx ;
       { DIP { DUP @inner } ; SWAP } ;
       { DIP { DUP @ctx } ; SWAP } ;
       EXEC ;
       IF_NONE { UNIT ; FAILWITH } { DUP @v ; DIP { DROP } } ;
       DIP { DROP } ;
       DIP { DROP } ;
       NIL operation ;
       PAIR ;
       DIP { DROP 2 } };
